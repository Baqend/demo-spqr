{
  "name": "spqr",
  "version": "1.0.0",
  "description": "Angular4 - Uses Webpack + TypeScript to compile on the fly - Includes binding, directives, http, pipes, and DI usage.",
  "main": "index.html",
  "dependencies": {
    "@angular/common": "^4.1.2",
    "@angular/compiler": "^4.1.2",
    "@angular/core": "^4.1.2",
    "@angular/forms": "^4.1.2",
    "@angular/platform-browser": "^4.1.2",
    "@angular/platform-browser-dynamic": "^4.1.2",
    "@angular/router": "^4.1.2",
    "baqend": "^2.7.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.4.0",
    "zone.js": "^0.8.10"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.3",
    "html-webpack-plugin": "^2.28.0",
    "raw-loader": "^0.5.1",
    "typescript": "^2.3.2",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  },
  "scripts": {
    "server": "webpack-dev-server --config webpack.config.js --inline --progress --profile --watch --hot --host 0.0.0.0",
    "dist": "webpack -p --progress --config webpack.config.js",
    "deploy": "baqend deploy -f dist spqr"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Baqend/demo-spqr.git"
  },
  "author": "Baqend",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Baqend/demo-spqr/issues"
  },
  "homepage": "https://github.com/Baqend/demo-spqr#readme"
}
